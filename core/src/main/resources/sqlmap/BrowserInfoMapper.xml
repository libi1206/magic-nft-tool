<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.libi.dao.BrowserInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.libi.bean.BrowserInfo">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="browser_serial" property="browserSerial" />
        <result column="bsc_addr" property="bscAddr" />
        <result column="bsc_words" property="bscWords" />
        <result column="bsc_pwd" property="bscPwd" />
        <result column="sol_addr" property="solAddr" />
        <result column="sol_words" property="solWords" />
        <result column="sol_pwd" property="solPwd" />
        <result column="proxy_addr" property="proxyAddr" />
        <result column="proxy_port" property="proxyPort" />
        <result column="proxy_username" property="proxyUsername" />
        <result column="proxy_pwd" property="proxyPwd" />
        <result column="browser_status" property="browserStatus" />
        <result column="last_time" property="lastTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, browser_serial, bsc_addr, bsc_words, bsc_pwd, sol_addr, sol_words, sol_pwd, proxy_addr, proxy_port, proxy_username, proxy_pwd, browser_status, last_time
    </sql>

</mapper>
